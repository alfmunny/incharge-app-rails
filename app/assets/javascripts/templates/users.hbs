<div class="container-fluid">
  <div class="row">
    <div class="col-md-3">
      <h2 style="bold">Users</h2>
      <ul class="list-group">
        {{#each}}
          {{#link-to 'user' this}}
            <li class="list-group-item">{{name}}</li>
          {{/link-to}}
        {{/each}}
      </ul>
    </div>
    <div class="col-md-6">
      {{outlet}}
    </div>
  </div>
</div>
