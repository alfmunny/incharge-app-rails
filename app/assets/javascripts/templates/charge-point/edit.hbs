<dl class="dl-horizontal">
  <dt>Charge Point Vender</dt>
  <dd>{{input valueBinding='chargePointVendor'}}</dd>
  <dt>Charge Point Model</dt>
  <dd>{{input valueBinding='chargePointModel'}}</dd>
  <dt>Firmware Version</dt>
  <dd>{{input valueBinding='firmwareVersion'}}</dd>
  <dt>Charge Point Serial Nr.</dt>
  <dd>{{input valueBinding='chargePointSerialNumber'}}</dd>
  <dt>ICCID</dt>
  <dd>{{input valueBinding='iccid'}}</dd>
  <dt>IMSI</dt>
  <dd>{{input valueBinding='imsi'}}</dd>
  <dt>Meter Type</dt>
  <dd>{{input valueBinding='meterType'}}</dd>
  <dt>Meter Serial Nr.</dt>
  <dd>{{input valueBinding='meterSerialNumber'}}</dd>
  <dt>Charge Box Serial Nr.</dt>
  <dd>{{input valueBinding='chargeBoxSerialNumber'}}</dd>
  <dt> Heartbeat Interval</dt>
  <dd>{{heartbeatInterval}}</dd>
</dl>
